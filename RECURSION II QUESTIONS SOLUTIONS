ANS:3--
SQRT OF A NUMBER--
class Solution {
    public static  int mySqrt(int x) 
    {
        if(x==0)
        {
            return x;
        }
        int i=1;
        int j=x;
       // int sqrt=0;
        while(i<=j)
        {
            int mid=i+(j-i)/2;
            if(x/mid==mid)
            {
                return mid;
            }
            else if(x/mid<mid)
            {
                j=mid-1;
            }
            else
            {
                i=mid+1;
                
            }

        }
        return j;
    }
    public static void main(String args[])
    {
        int x=9;
        System.out.println(mySqrt(x));
    }
}
